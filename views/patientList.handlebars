<div class="container text-center">
  <div class="row align-items-start">
    <div class="col list">
      <br><br><br>
      <h1 class="listTitle">Patient List by ID #</h1>
      {{#each patientmap as |content|}}
        <ol>
          <div class="listChart">
            <a href="/patientList/{{content.id}}"><button class="patientId">{{content.id}}</button></a>
            <p class="names">{{content.first_name}} {{content.last_name}}</p>
          </div>
        </ol>
      {{/each}}
    </div>
  </div>
</div>