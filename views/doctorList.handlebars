<div class="container text-center">
  <div class="row align-items-start">

    <div class="col list">
    <h1 class="listTitle">Doctor List by ID #</h1>
    {{#each doctormap as |doc|}}
    <ol>
    <div class="listChart">
    <a href="/doctor/{{id}}"><button class="patientId"> {{doc.id}}</button></a>
    <p class="names"> {{doc.first_name}} {{last_name}}</p>
    </div>
    </ol>
    {{/each}}
    </div>
  
  </div>
</div>